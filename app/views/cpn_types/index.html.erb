<p id="notice"><%= notice %></p>

<h1>Listing Cpn Types</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cpn_types.each do |cpn_type| %>
      <tr>
        <td><%= cpn_type.id %></td>
        <td><%= cpn_type.name %></td>
        <td><%= link_to 'Show', cpn_type %></td>
        <td><%= link_to 'Edit', edit_cpn_type_path(cpn_type) %></td>
        <td><%= link_to 'Destroy', cpn_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cpn type', new_cpn_type_path %>
