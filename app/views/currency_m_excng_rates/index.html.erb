<p id="notice"><%= notice %></p>

<h1>Listing Currency M Excng Rates</h1>

<table>
  <thead>
    <tr>
      <th>Currency cd</th>
      <th>Exchng rate ver</th>
      <th>Exchng rate</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Rgstr dt</th>
      <th>Rgstr usr</th>
      <th>Lst upd dt</th>
      <th>Lst upd usr</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @currency_m_excng_rates.each do |currency_m_excng_rate| %>
      <tr>
        <td><%= currency_m_excng_rate.currency_cd %></td>
        <td><%= currency_m_excng_rate.exchng_rate_ver %></td>
        <td><%= currency_m_excng_rate.exchng_rate %></td>
        <td><%= currency_m_excng_rate.start_date %></td>
        <td><%= currency_m_excng_rate.end_date %></td>
        <td><%= currency_m_excng_rate.rgstr_dt %></td>
        <td><%= currency_m_excng_rate.rgstr_usr %></td>
        <td><%= currency_m_excng_rate.lst_upd_dt %></td>
        <td><%= currency_m_excng_rate.lst_upd_usr %></td>
        <td><%= link_to 'Show', currency_m_excng_rate %></td>
        <td><%= link_to 'Edit', edit_currency_m_excng_rate_path(currency_m_excng_rate) %></td>
        <td><%= link_to 'Destroy', currency_m_excng_rate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

