<h1>Listing Prd M Set Contents</h1>

<table>
  <thead>
    <tr>
      <th>Sku</th>
      <th>Sku content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prd_m_set_contents.each do |prd_m_set_content| %>
      <tr>
        <td><%= prd_m_set_content.sku %></td>
        <td><%= prd_m_set_content.sku_content %></td>
        <td><%= link_to 'Show', prd_m_set_content %></td>
        <td><%= link_to 'Edit', edit_prd_m_set_content_path(prd_m_set_content) %></td>
        <td><%= link_to 'Destroy', prd_m_set_content, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

