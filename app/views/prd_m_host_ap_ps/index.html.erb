<h1>Listing Prd M Host Apps</h1>

<table>
  <thead>
    <tr>
      <th>Sku</th>
      <th>App cd</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prd_m_host_apps.each do |prd_m_host_app| %>
      <tr>
        <td><%= prd_m_host_app.SKU %></td>
        <td><%= prd_m_host_app.APP_CD %></td>
        <td><%= link_to 'Show', prd_m_host_app %></td>
        <td><%= link_to 'Edit', edit_prd_m_host_app_path(prd_m_host_app) %></td>
        <td><%= link_to 'Destroy', prd_m_host_app, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

