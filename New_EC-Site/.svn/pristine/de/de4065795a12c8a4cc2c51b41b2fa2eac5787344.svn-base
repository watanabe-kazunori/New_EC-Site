<p id="notice"><%= notice %></p>

<h1>クーポンマスタ管理画面</h1>

<style type="text/css">
.table1 {
  border-collapse: collapse;
}
.table1 th {
  background-color: #cccccc;
}
</style>
<table class="table1" border=1>
  <thead>
    <tr>
      <th nowrap>クーポンID</th>
      <th nowrap>クーポンコード</th>
      <th nowrap>クーポン効力</th>
      <th nowrap>タイトル</th>
      <th nowrap>クーポンクラス</th>
      <th nowrap>クーポン値引タイプ</th>
      <th nowrap>ベンダー仕切クーポン</th>
      <th nowrap>使用回数制限</th>
      <th nowrap>有効期限開始日時</th>
      <th nowrap>有効期限終了日時</th>
      <th nowrap>割引額</th>
      <th nowrap>割引率</th>
      <th nowrap>対象製品条件</th>
      <th nowrap>対象除外製品条件</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cpn_ms.each do |cpn_m| %>
      <tr>
        <td><%= cpn_m.CPN_ID %></td>
        <td><%= cpn_m.CPN_CD %></td>
        <td><%= cpn_m.CPN_ST %></td>
        <td><%= cpn_m.CPN_TITLE %></td>
        <td><%= cpn_m.CPN_CLS %></td>
        <td><%= cpn_m.CPN_DISCNT_TYPE %></td>
        <td><%= cpn_m.VNDR_CPN %></td>
        <td><%= cpn_m.USE_LIMIT %></td>
        <td><%= cpn_m.START_DT %></td>
        <td><%= cpn_m.EXPIRATION_DT %></td>
        <td><%= cpn_m.DSCNT_VALUE %></td>
        <td><%= cpn_m.DSCNT_RATE %></td>
        <td><%= cpn_m.TRGT_PRD_CNDTN %></td>
        <td><%= cpn_m.EXCLD_PRD_CNDTN %></td>
        <td><%= link_to 'Show', cpn_m %></td>
        <td><%= link_to 'Edit', edit_cpn_m_path(cpn_m) %></td>
        <td><%= link_to 'Destroy', cpn_m, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cpn m', new_cpn_m_path %>
